config_version=5

[application]

config/name="GDClaw"
run/main_scene="res://game.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://sprites/objects/pickup/restore/extra_life/frame007.png"

[autoload]

Utils="*res://utils.gd"
Settings="*res://settings.gd"

[display]

window/size/viewport_width=800
window/stretch/mode="2d"

[editor_plugins]

enabled=PackedStringArray("res://addons/inventory_manager/plugin.cfg")

[global]

vsync=true

